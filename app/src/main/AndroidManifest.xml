<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.home.mybid">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyBid">
        <activity android:name="com.mybid.activities.AddItem"><intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter></activity>
        <activity android:name="com.mybid.activities.ItemActivity" />
        <activity android:name="com.mybid.activities.UserActivity" />
        <activity android:name="com.mybid.activities.RegisterActivity"></activity>
        <activity
            android:name="com.mybid.activities.LoginActivity"></activity>

        <activity android:name="com.mybid.activities.StartActivity">

        </activity>
    </application>

</manifest>